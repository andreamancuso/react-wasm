{
    "name": "@react-wasm/dear-imgui",
    "version": "0.1.31-canary-001",
    "main": "dist/index.js",
    "files": [
        "dist/*"
    ],
    "homepage": "https://github.com/andreamancuso/react-wasm#readme",
    "license": "MIT",
    "author": {
        "name": "Andrea Mancuso",
        "url": "https://github.com/andreamancuso"
